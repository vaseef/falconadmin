{
    "routes": [
      {
        "route": "/static/*",
        "headers": {
          "Cache-Control": "public, max-age=31536000, immutable"
        }
      },
      {
        "route": "/images/*",
        "headers": {
          "Cache-Control": "public, max-age=31536000, immutable"
        }
      },
      {
        "route": "/build/*",
        "headers": {
          "Cache-Control": "public, max-age=31536000, immutable"
        }
      },
      {
        "route": "/*",
        "rewrite": "/index.html"
      }
    ]
}


  